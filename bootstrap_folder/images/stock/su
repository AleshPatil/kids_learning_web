<script>

    function call_emailajax() {
        var e = $("#txtemail").val();
        if (1 == checkemail(e)) {
            $.ajax({
                url: "/subscribers/updatemaillist",
                data: { id: e },
                cache: !1,
                type: "POST",
                success: function (e) {
                    if ("yes" == e)
                        ($("#sign_up_form").display = "none"),
                        ($("#div_msg").display = "block"),
                        $("#div_msg").show(),
                        $("#div_msg1").hide(),
                        $("#txtemail").val(""),
                        ($("#div_msg1").display = "none"),
                        $("#div_msg").html("<i class='fa fa-check-circle'></i> Thank you for subscribing!");
                    else if ("exists" == e)
                        return (
                        ($("#div_msg").display = "block"),
                        $("#div_msg").show(),
                        $("#div_msg1").hide(),
                        ($("#div_msg1").display = "none"),
                        $("#div_msg").html("<i class='fa fa-check-circle'></i> You have already subscribed. Thank you!"),
                        $("#txtemail").val(""),
                        !1
                        );
                },
            });
        }
    }
    function checkemail(e) {
        if ("" == e)
            return (
            ($("#div_msg1").display = "block"), $("#div_msg1").show(), $("#div_msg").hide(), ($("#div_msg").display = "none"), $("#div_msg1").html(" <i class='fa fa-times-circle'></i> Please enter you email id"), $("#txtemail").focus(), !1
            );
        return (
        !!e.match(/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{3,6})?)$/i) ||
        (($("#div_msg").display = "block"),
        $("#div_msg1").show(),
        $("#div_msg").hide(),
        ($("#div_msg").display = "none"),
        $("#div_msg1").html("<i class='fa fa-times-circle'></i> Please enter a valid email address"),
        $("#txtemail").val(""),
        !1)
        );
    }
    function submit_md_click() {
        return "Type something..." == $("#text_md").val() || "" == $("#text_md").val()
        ? (alert("Please enter search keyword"), !1)
        : $("#text_md").val().match("^(?=.*[~`^<>:;.!&+])")
        ? (alert("Please enter valid characters. The following characters are not accepted ` ~ ^ < > : ; . ! & +"), !1)
        : void (window.location = "/search/" + $("#text_md").val());
    }
    function keyDownSubmit() {
        13 == event.keycode && document.getelementbyid("submit_md").click();
    }
    function key1() {
        if (validateSearch()) return (window.location = "/search/" + $("#text_md").val()), !0;
    }
    function validateSearch() {
        if ("Type something..." == $("#text_md").val() || "" == $("#text_md").val()) return alert("Please enter search keywords"), !1;
        return !$("#text_md").val().match("^(?=.*[~`^<>:;.!&])") || (alert("Please enter valid characters. The following characters are not accepted ` ~ ^ < > : ; . ! &"), !1);
    }


</script>
    
