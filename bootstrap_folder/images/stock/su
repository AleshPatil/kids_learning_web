            $(document).ready(function () {
                var strfloorid = 2;
                // alert($("#firsttab").val());

                // alert($("#txtCount").val());
                for (var i = 1; i <= $("#txtCount").val() ; i++) {
                    var strdivname = "#div" + i;
                    // alert(strdivname);
                    $(strdivname).hide().css("visibility", "hidden");
                    //if (i == 2) {
                    //    showboxes(i);
                    //    $("a[href='#2']").addClass("active");

                    //}
                }
                if ($("#firsttab").val() == 1) {
                    strfloorid = $("#floor_id").val();
                    $("#txtfloorId").val(strfloorid);

                }
                // alert(strfloorid);
                showboxes(strfloorid);
                var strhref = "a[href='#" + strfloorid + "']";

                // alert(strhref);
                $(strhref).addClass("active");

                $("#ddnoOffloors").change(function () {
                    var ddtext = $("#ddnoOffloors option:selected").val();

                 //   alert(ddtext);

                    var strhref = "a[href='#" + ddtext + "']";
                    $(strhref).addClass("active");
                    //  alert("hidden value set up" + ddtext);
                    showboxes(ddtext);

                })



                //$('a').click(function () {
                //    alert("in a classss");
                //        // ADD CLASS TO ALL THE TAGS.
                //    $('a').removeClass('active');
                //    if ($(this).hasClass('cal-btn scroll-down')) {    // CHECK IF THE TAG HAS 'in-active' CLASS.

                //        $(this)

                //            .addClass('active');
                //    }
                //})



            });



            function CheckData() {

              //  debugger;
                // alert(year);
                var strid = $("#floor_id").val()
                // alert($("#floor_id").val());


                if ($('#txtfloorId').length > 0) {
                    //alert($('#txtfloorId').length);
                    strid = $("#txtfloorId").val();
                }
                // alert($("#SecondTab").val());

                var numchk = /^[0-9]*$/;
                var isValid = true;
                for (var i = 1; i <= strid; i++) {
                    if (i == 1 && ($("#txt_1").val().length <= 0 || $("#txt_1").val() == 0)) {
                        $("#err1").empty()
                        $("#err1").append("<font color=\"red\">Please enter area in Sq. ft.</font>");
                        isValid = false;
                    } else {
                        if (numchk.test($("#txt_1").val()) == false) {
                            $("#err1").empty()
                            $("#err1").append("<font color=\"red\">Please enter numbers only</font>");
                            isValid = false;
                        }
                        if ($("#txt_1").val().length > 6) {
                            $("#err1").empty()
                            $("#err1").append("<font color=\"red\">Please enter valid numbers</font>");
                            isValid = false;
                        }
                    }

                    if (i == 2 && ($("#txt_2").val().length <= 0 || $("#txt_2").val() == 0)) {
                        $("#err2").empty()
                        $("#err2").append("<font color=\"red\">Please enter area in Sq. ft.</font>");
                        isValid = false;
                    }
                    else {
                        if (numchk.test($("#txt_2").val()) == false) {
                            $("#err2").empty()
                            $("#err2").append("<font color=\"red\">Please enter numbers only</font>");
                            isValid = false;
                        }

                        if ($("#txt_2").val().length > 6) {
                            $("#err2").empty()
                            $("#err2").append("<font color=\"red\">Please enter valid numbers</font>");
                            isValid = false;
                        }
                    }
                    if (i == 3 && ($("#txt_3").val().length <= 0 || $("#txt_3").val() == 0)) {
                        $("#err3").empty()
                        $("#err3").append("<font color=\"red\">Please enter area in Sq. ft.</font>");
                        isValid = false;
                    }
                    else {
                        if (numchk.test($("#txt_3").val()) == false) {
                            $("#err3").empty()
                            $("#err3").append("<font color=\"red\">Please enter numbers only</font>");
                            isValid = false;
                        }
                        if ($("#txt_3").val().length > 6) {
                            $("#err3").empty()
                            $("#err3").append("<font color=\"red\">Please enter valid numbers</font>");
                            isValid = false;
                        }
                    }
                    if (i == 4 && ($("#txt_4").val().length <= 0 || $("#txt_4").val().length == 0)) {
                        $("#err4").empty()
                        $("#err4").append("<font color=\"red\">Please enter area in Sq. ft. </font>");
                        isValid = false;
                    }
                    else {

                        if (numchk.test($("#txt_4").val()) == false) {
                            $("#err4").empty()
                            $("#err4").append("<font color=\"red\">Please enter numbers only</font>");
                            isValid = false;
                        }
                        if ($("#txt_4").val().length > 6) {
                            $("#err4").empty()
                            $("#err4").append("<font color=\"red\">Please enter valid numbers</font>");
                            isValid = false;
                        }
                    }
                }
                //  console.log(isValid);
                // alert(isValid);
                if (isValid == true) {
                    var strG = $("#txt_1").val();
                    var strF = $("#txt_2").val();
                    var strS = $("#txt_3").val();
                    var strT = $("#txt_4").val();
                    var strfloor_expanded_id = $("#floor_expanded_id").val();
                    var strcon_stage_Id = $("#con_stage_Id").val()
                    if (strG.length <= 0) {
                        strG = 0;
                    }
                    if (strF.length <= 0) {
                        strF = 0;
                    }
                    if (strS.length <= 0) {
                        strS = 0;
                    }
                    if (strT.length <= 0) {
                        strT = 0;
                    }
                    if (strfloor_expanded_id.length <= 0) {
                        strfloor_expanded_id = 0;
                    }
                    if (strcon_stage_Id.length <= 0) {
                        strcon_stage_Id = 0;
                    }
                }
                // }
                //else
                //{
                //   // alert("in");
                //    var strG = $("#g").val();
                //    var strF = $("#f").val();
                //    var strS = $("#s").val();
                //    var strT = $("#t").val();
                //    var strfloor_expanded_id = $("#floor_expanded_id").val();
                //    var strcon_stage_Id = $("#con_stage_Id").val()


                //    if (strG.length <= 0) {
                //        strG = 0;
                //    }
                //    if (strF.length <= 0) {
                //        strF = 0;
                //    }
                //    if (strS.length <= 0) {
                //        strS = 0;
                //    }
                //    if (strT.length <= 0) {
                //        strT = 0;
                //    }
                //    if (strfloor_expanded_id.length <= 0) {
                //        strfloor_expanded_id = 0;
                //    }
                //    if (strcon_stage_Id.length <= 0) {
                //        strcon_stage_Id = 0;
                //    }

                //}
                // alert("ajax");
                //   alert(strG);
                // alert(strF);
                //alert(strS);
                // alert(strT);
                var strfloor = $("#txtfloorId").val();
                var strcmt_user_value = $("#cmt_user_value").val();
                var strsteel_user_value = $("#steel_user_value").val();
                var strsand_user_value = $("#sand_user_value").val();
                var strcaCft_user_value = $("#caCft_user_value").val();
                var strbricks_user_value = $("#bricks_user_value").val();
                // alert(strfloor);
                $.ajax({
                    type: "post",
                    url: "/ProductRange/GetMaterialEstimate",
                    //dataType: "html",
                    // data: { id: 11 },
                    data: { g: strG, f: strF, s: strS, t: strT, floor_id: strfloor, floor_Expandedid: strfloor_expanded_id, con_stage_Id: strcon_stage_Id, cmt_user_value: strcmt_user_value, steel_user_value: strsteel_user_value, sand_user_value: strsand_user_value, caCft_user_value: strcaCft_user_value, bricks_user_value: strbricks_user_value },
                    contentType: "application/x-www-form-urlencoded;charset=utf-8",
                    //  async: true,
                    success: function (data) {
                        //  alert(data);
                        $("#Tabboady").empty();
                        $("#Tabboady").append(data.ModelString);

                        bindFunction();
                        // alert(ajaxModelCount + " ajaxModelCount");


                    },
                    error: function (xhr, status, error) {

                        console.log(xhr.responseText);
                        //alert("message : \n" +
                        //    "An error occurred, for more info check the js console" +
                        //    "\n status : \n" + status + " \n error : \n" + error);
                    },
                    complete: function (xhr, status, error) {
                        console.log(xhr.responseText);
                        //  $("#scriptdiv").empty();

                        // alert("message on complete : \n" +
                        // "An error occurred, for more info check the js console" +
                        //  "\n status : \n" + status + " \n error : \n" + error);
                    }

                });
            }




            function showboxes(strid) {

               // debugger;
                //  alert("showbox"+strid);
                //  alert($("#txtCount").val());
                //$("#frm1 input").each(function () {
                //    if ($(this).is('[type=text]') && !parseInt($(this).val())) {
                //        $(this).val('');
                //    }
                //});
                //  alert(strid);
                $("#txtfloorId").val(strid);
                $("#floor_id").val(strid)
                $("#ddnoOffloors").val(strid);
                //  alert("changed floor"+$("#txtfloorId").val());
                $("a[href='#2']").removeClass("active");

                for (var i = 1; i <= $("#txtCount").val() ; i++) {
                    var strdivname = "#div" + i;
                    //   alert(strdivname);
                    var strhref = "a[href='#" + i + "']";
                    $(strhref).removeClass("active");
                    $(strdivname).hide().css("visibility", "hidden");
                }
                for (var i = strid + 1; i <= $("#txtCount").val() ; i++) {

                    var strtxtname = "#txt_" + i;
                    //   alert("namevale"+$(strtxtname).val());

                    $(strtxtname).val('');

                }
                for (var i = 1; i <= strid; i++) {
                    var strdivname = "#div" + i;
                    //   alert(strdivname);
                    $(strdivname).show().css("visibility", "visible");
                }
                var strhref = "a[href='#" + strid + "']";

                // alert(strhref);
                $(strhref).addClass("active");
            }

            function showalert() {

                ///;
                // alert($("#txtfloorId").val());

                // alert($("#txt_1").val());

                // e.preventDefault();

                //alert("in");
                $.ajax({
                    type: "post",
                    url: "/CostCalculater/GetMaterialEstimate",
                    //dataType: "html",
                    //  data: { g: $("#txt_1").val(), f: $("#txt_2").val(), s: $("#txt_3").val(), t: $("#txt_4").val(), floor_id: $("#txtfloorId").val() },
                    data: { floor_id: 4 },
                    //ContentType: application/json,
                    contentType: "application/x-www-form-urlencoded;charset=utf-8",
                    async: true,
                    success: function (data) {
                        // alert(data);

                        $("#Tabboady").empty();
                        $("#Tabboady").append(data.ModelString);


                        //if (ajaxModelCount >= hdnpageCount) {
                        //  //  $("#loadMore").remove();
                        //    // $("#loadMore").hide().fadeOut(1000);
                        //}


                    },
                    error: function (xhr, status, error) {

                        console.log(xhr.responseText);
                        alert("message on error : \n" +
                            "An error occurred, for more info check the js console" +
                            "\n status : \n" + status + " \n error : \n" + error);
                    }
                    , failure: function (response) {
                        alert(response.d);
                    },
                    complete: function (xhr, status, error) {
                        console.log(xhr.responseText);
                        //   alert("message on complete : \n" +
                        //      "An error occurred, for more info check the js console" +
                        //     "\n status : \n" + status + " \n error : \n" + error);
                    }

                });

            }


            function Chcektextboxdata() {

                var strid = $("#txtfloorId").val();

                //  alert(strid);
                for (var i = 1 ; i <= strid ; i++) {

                    var strtxtname = "#txt_" + i;
                    //  alert(strtxtname);
                    if ($(strtxtname).val().length <= 0) {
                        return false;
                    }
                }

            }
            function ShowData() {

              //  debugger;

                if ($("#txtfloorId").val() == "") {
                    //$("#<font color="red">")
                    $("#diverror").empty()
                    $("#diverror").append("<font color=\"red\">Please select no of floors</font>");
                    //alert("Please select no of floors");
                }
                else if (Chcektextboxdata() == false) {
                    $("#diverror").empty()
                    $("#diverror").append("<font color=\"red\">Please enter area.</font>");
                }
                else {

                    var strcmt_user_value = 0;
                    var strsteel_user_value = 0;
                    var strsand_user_value = 0;
                    var strcaCft_user_value = 0;
                    var strbricks_user_value = 0;
                    //alert("in");
                    $.ajax({
                        type: "post",
                        url: "/CostCalculater/GetMaterialEstimate",
                        //dataType: "html",
                        data: { g: $("#txt1").val(), f: $("#txt2").val(), s: $("#txt3").val(), t: $("#txt4").val(), floor_id: $("#txtfloorId").val(), con_stage_Id: strcon_stage_Id, cmt_user_value: strcmt_user_value, steel_user_value: strsteel_user_value, sand_user_value: strsand_user_value, caCft_user_value: strcaCft_user_value, bricks_user_value: strbricks_user_value },
                        contentType: "application/x-www-form-urlencoded;charset=utf-8",
                        //  async: true,
                        success: function (data) {
                            // alert(data);

                            $("#Tabboady").empty();
                            $("#Tabboady").append(data.ModelString);


                            //if (ajaxModelCount >= hdnpageCount) {
                            //  //  $("#loadMore").remove();
                            //    // $("#loadMore").hide().fadeOut(1000);
                            //}


                        },
                        error: function (xhr, status, error) {

                            console.log(xhr.responseText);
                            alert("message : \n" +
                                "An error occurred, for more info check the js console" +
                                "\n status : \n" + status + " \n error : \n" + error);
                        }
                    });

                }

            }

            function LoadMaterialTab() {
                //  alert("second tab" + $("#SecondTab").val())
                if ($("#SecondTab").val() != 0) {
                    //  alert("second tab" + $("#SecondTab").val())
                    CheckData();
                }
            }
            function LoadCostData() {

              //  debugger;
                //   alert("in");
                if ($("#SecondTab").val() != 0) {
                    var txtFloorExpanded = $("#floor_expanded_id").val();
                    var con_stage_ID = $("#con_stage_ID").val();


                    if ($('#ddExpantedFloors').length) {
                        txtFloorExpanded = $("#ddExpantedFloors option:selected").val();
                    }
                    if ($('#ddconstructionStage').length) {
                        con_stage_ID = $("#ddconstructionStage option:selected").val();
                    }
                    //  alert(txtFloorExpanded);
                    //  alert(con_stage_ID);
                    //  alert(Chcektextboxdata());
                    if (txtFloorExpanded == '') {
                        txtFloorExpanded = 0;

                    }

                    if (con_stage_ID == '') {
                        con_stage_ID = 0;

                    }

                    if ($("#txtfloorId").val() == "") {
                        //$("#<font color="red">")
                        $("#diverror").empty()
                        $("#diverror").append("<font color=\"red\"> Please select no of floors</font>");
                        //alert("Please select no of floors");
                    }
                    else if (Chcektextboxdata() == false) {
                        $("#diverror").empty()
                        $("#diverror").append("<font color=\"red\">Please enter area.</font>");
                    }
                    else {

                        var strcmt_user_value = $("#cmt_user_value").val();
                        var strsteel_user_value = $("#steel_user_value").val();
                        var strsand_user_value = $("#sand_user_value").val();
                        var strcaCft_user_value = $("#caCft_user_value").val();
                        var strbricks_user_value = $("#bricks_user_value").val();
                        $.ajax({
                            type: "post",
                            url: "/ProductRange/GetToNextScreen",
                            //dataType: "html",
                            // data: { id: 11 },
                            data: { g: $("#g").val(), f: $("#f").val(), s: $("#s").val(), t: $("#t").val(), floor_id: $("#floor_id").val(), floor_Expandedid: $("#floor_expanded_id").val(), con_stage_Id: $("#con_stage_Id").val(), cmt_user_value: strcmt_user_value, steel_user_value: strsteel_user_value, sand_user_value: strsand_user_value, caCft_user_value: strcaCft_user_value, bricks_user_value: strbricks_user_value },
                            contentType: "application/x-www-form-urlencoded;charset=utf-8",
                            //  async: true,
                            success: function (data) {
                                // alert(data);
                                $("#MainDiv").empty();
                                // $("#ScriptDiv").empty();
                                $("#MainDiv").append(data.ModelString);


                                // alert(ajaxModelCount + " ajaxModelCount");



                            },
                            error: function (xhr, status, error) {

                                console.log(xhr.responseText);
                                alert("message : \n" +
                                    "An error occurred, for more info check the js console" +
                                    "\n status : \n" + status + " \n error : \n" + error);
                            },
                            complete: function (xhr, status, error) {
                                console.log(xhr.responseText);
                                // alert("message on complete : \n" +
                                //   "An error occurred, for more info check the js console" +
                                //   "\n status : \n" + status + " \n error : \n" + error);
                            }
                        });
                    }

                }
                else {

                    alert("please select material quantity details");
                }
            }

        </script>
    </div>

